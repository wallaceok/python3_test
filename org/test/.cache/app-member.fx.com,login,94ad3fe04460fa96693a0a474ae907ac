    $(document).ready(function () {
      // 勿删 -- 开始
      var attrAction = $('form').attr('action') + '?width=' + window.screen.width + '&height=' + window.screen.height;
      $('form').attr('action', attrAction);
      // 勿删 -- 结束

      var i = 0
      setInterval(function () {
        if (i == 0) {
          i = 1
          $('.lantern1,.lantern4').removeClass('zhuan_left').addClass('zhuan_right')
          $('.lantern2,.lantern3').addClass('zhuan_left').removeClass('zhuan_right')
        } else {
          i = 0
          $('.lantern1,.lantern4').addClass('zhuan_left').removeClass('zhuan_right')
          $('.lantern2,.lantern3').removeClass('zhuan_left').addClass('zhuan_right')
        }
      }, 1000)
    })

